<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Nombre" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#60AA9B</item>
        <item name="colorPrimaryVariant">#60AA9B</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#60AA9B</item>
        <item name="colorSecondaryVariant">#60AA9B</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#60AA9B</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Nombre.NoActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Nombre.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Nombre.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>